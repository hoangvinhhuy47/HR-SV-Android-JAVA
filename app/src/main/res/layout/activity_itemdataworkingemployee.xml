<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/Card_2"
    android:layout_width="match_parent"
    android:layout_height="@dimen/heith_Item_workingemployee"
    android:layout_marginTop="@dimen/Margin_padding5"
    app:cardCornerRadius="10dp">

    <RelativeLayout
        android:background="@drawable/backgroud_home"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="@dimen/Margin_padding5">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:orientation="vertical">;

            <GridLayout
                android:id="@+id/employee"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:columnCount="2"
                android:rowCount="1">

                <Button
                    android:id="@+id/dateworkingE"
                    android:layout_width="@dimen/width_button_date_Item_workingemployee"
                    android:layout_height="@dimen/Margin_padding45"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="@dimen/Margin_padding3"
                    android:background="@drawable/textboxaccount"
                    android:text="T2\n00/00/2000"
                    android:textColor="@color/mautrang"
                    android:textSize="@dimen/TextSize12"></Button>

                <GridLayout
                    android:layout_width="@dimen/width_group_employee_Item_workingemployee"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:alignmentMode="alignMargins"
                    android:columnCount="6"
                    android:rowCount="2">

                    <TextView
                        android:layout_width="@dimen/Margin_padding35"
                        android:layout_height="@dimen/Margin_padding35"
                        android:background="#C8FBE1"
                        android:gravity="center"
                        android:text="W"
                        android:textColor="@color/mauxanh"
                        android:textSize="@dimen/TextSize11"></TextView>

                    <TextView
                        android:layout_width="@dimen/Margin_padding35"
                        android:layout_height="@dimen/Margin_padding35"
                        android:background="#C8FBE1"
                        android:gravity="center"
                        android:text="P"
                        android:textColor="@color/mauxanh"
                        android:textSize="@dimen/TextSize11"></TextView>

                    <TextView
                        android:layout_width="@dimen/Margin_padding35"
                        android:layout_height="@dimen/Margin_padding35"
                        android:background="#C8FBE1"
                        android:gravity="center"
                        android:text="K"
                        android:textColor="@color/mauxanh"
                        android:textSize="@dimen/TextSize11"></TextView>

                    <TextView
                        android:layout_width="@dimen/Margin_padding37"
                        android:layout_height="@dimen/Margin_padding35"
                        android:background="#C8FBE1"
                        android:gravity="center"
                        android:text="OT1"
                        android:textColor="@color/mauxanh"
                        android:textSize="@dimen/TextSize11"></TextView>

                    <TextView
                        android:layout_width="@dimen/Margin_padding40"
                        android:layout_height="@dimen/Margin_padding35"
                        android:background="#C8FBE1"
                        android:gravity="center"
                        android:text="OT150"
                        android:textColor="@color/mauxanh"
                        android:textSize="@dimen/TextSize11"></TextView>

                    <TextView
                        android:layout_width="@dimen/Margin_padding45"
                        android:layout_height="@dimen/Margin_padding35"
                        android:background="#C8FBE1"
                        android:gravity="center"
                        android:text="OTĐêm"
                        android:textColor="@color/mauxanh"
                        android:textSize="@dimen/TextSize11"></TextView>

                    <TextView
                        android:id="@+id/timework"
                        android:layout_width="@dimen/Margin_padding35"
                        android:layout_height="@dimen/Margin_padding35"
                        android:background="#ECECEC"
                        android:gravity="center"
                        android:text="9999"
                        android:textColor="@color/mauxanh"
                        android:textSize="@dimen/TextSize13"></TextView>

                    <TextView
                        android:id="@+id/timeleaveP"
                        android:layout_width="@dimen/Margin_padding35"
                        android:layout_height="@dimen/Margin_padding35"
                        android:background="#ECECEC"
                        android:gravity="center"
                        android:text="999"
                        android:textColor="@color/mauxanh"
                        android:textSize="@dimen/TextSize13"></TextView>

                    <TextView
                        android:id="@+id/timeleaveN"
                        android:layout_width="@dimen/Margin_padding35"
                        android:layout_height="@dimen/Margin_padding35"
                        android:background="#ECECEC"
                        android:gravity="center"
                        android:text="999"
                        android:textColor="@color/mauxanh"
                        android:textSize="@dimen/TextSize13"></TextView>

                    <TextView
                        android:id="@+id/timeOT1"
                        android:layout_width="@dimen/Margin_padding37"
                        android:layout_height="@dimen/Margin_padding35"
                        android:background="#ECECEC"
                        android:gravity="center"
                        android:text="999"
                        android:textColor="@color/mauxanh"
                        android:textSize="@dimen/TextSize13"></TextView>

                    <TextView
                        android:id="@+id/timeOT150"
                        android:layout_width="@dimen/Margin_padding40"
                        android:layout_height="@dimen/Margin_padding35"
                        android:background="#ECECEC"
                        android:gravity="center"
                        android:text="999"
                        android:textColor="@color/mauxanh"
                        android:textSize="@dimen/TextSize13"></TextView>

                    <TextView
                        android:id="@+id/timeOTNight"
                        android:layout_width="@dimen/Margin_padding45"
                        android:layout_height="@dimen/Margin_padding35"
                        android:background="#ECECEC"
                        android:gravity="center"
                        android:text="999"
                        android:textColor="@color/mauxanh"
                        android:textSize="@dimen/TextSize13"></TextView>
                </GridLayout>
            </GridLayout>
        </LinearLayout>
    </RelativeLayout>
</androidx.cardview.widget.CardView>